                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": false,
                "clear": true
            },
            "problemMatcher": {
                "owner": "cpp",
                "fileLocation": [
                    "relative",
                    "${workspaceFolder}/MDK-ARM"
                ],
                "pattern": {
                    "regexp": "^(.*)\\((\\d*)\\):\\s*(.rror|.arning|.nfo):\\s*(.*)$",
                    "file": 1,
                    "line": 2,
                    "severity": 3,
                    "message": 4,
                    "column": -1
                }
            },
            "group": "build"
