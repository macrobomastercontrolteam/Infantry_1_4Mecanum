            "command": "C:\\msys64\\ucrt64\\bin\\gcc.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${fileDirname}\\circular_buffer.c",
                "${fileDirname}\\application\\gimbal_task.c",
                "${fileDirname}\\application\\usb_task.c",
                "${fileDirname}\\components\\algorithm\\user_lib.c",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe",
                "-Wall"
            ],
            "options": {
                "cwd": "C:\\msys64\\ucrt64\\bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
