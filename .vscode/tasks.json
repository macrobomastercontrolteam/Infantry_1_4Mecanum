    "version": "2.0.0",
    "tasks": [
        {
            "label": "Import settings from Keil's v4 or v5 project",
            "type": "shell",
            "command": "python",
            "args": [
                "Scripts/_parse_Keil_project.py",
                "${relativeFile}",
                ".vscode/c_cpp_properties.json",
                "Scripts/_set_Keil_project_paths.bat"
            ],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": false,
                "clear": true
            },
            "problemMatcher": [
                "$tsc"
            ],
            "group": "build"
        },
        {
            "label": "Build by Keil",
